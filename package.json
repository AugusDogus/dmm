{
  "name": "deadlock-mod-manager",
  "version": "1.0.0",
  "description": "A simple mod manager for Deadlock",
  "main": "./out/main/index.js",
  "author": "AugusDogus",
  "homepage": "https://store.steampowered.com/app/1422450/Deadlock/",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-router": "^1.81.14",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "10.45.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "electron-debug": "^4.1.0",
    "electron-settings": "^4.0.4",
    "electron-trpc": "^0.6.1",
    "electron-updater": "^6.1.7",
    "lucide-react": "^0.314.0",
    "react-intersection-observer": "^9.13.1",
    "steam-game-path": "^2.3.0",
    "superjson": "^1.12.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/router-devtools": "^1.81.14",
    "@tanstack/router-plugin": "^1.81.9",
    "@types/node": "^18.19.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwind": "^2.2.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shadcn-ui": "^0.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "patchedDependencies": {
    "steam-game-path@2.3.0": "patches/steam-game-path@2.3.0.patch",
    "electron-settings@4.0.4": "patches/electron-settings@4.0.4.patch"
  }
}
